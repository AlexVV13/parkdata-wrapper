  "keywords": [
    "themepark",
    "parkdata",
    "wait-times",
    "attractions",
    "rollercoaster",
    "parks-api"
  ],
  "author": "AlexVV",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexVV/parkdata.git"
  },
  "bugs": {
    "url": "https://github.com/AlexVV/parkdata/issues"
  },
  "homepage": "https://github.com/AlexVV/parkdata#readme",
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "sideEffects": false,
  "dependencies": {
    "adm-zip": "^0.5.16",
    "axios": "^1.12.2",
    "better-sqlite3": "^12.4.1",
    "moment-timezone": "^0.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "docdash": "^2.0.2",
    "eslint": "^8.57.1",
    "eslint-config-google": "^0.14.0",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "jsdoc": "^4.0.4"
  }
}
