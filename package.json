{
  "name": "parkdata",
  "version": "1.0.0",
  "description": "Wrapper to fetch theme park data in readable JSON",
  "type": "module",
  "main": "./index.js",
  "exports": {
    ".": "./index.js"
  },
  "files": [
    "index.js",
    "lib/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --verbose",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "demo": "node demo/parcAsterix-demo.js",
    "docs": "jsdoc -c jsdoc.json",
    "prepare": "npm run lint"
